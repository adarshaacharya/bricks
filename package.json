    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.6.0",
    "handlebars": "^4.7.8",
    "install": "^0.13.0",
    "ioredis": "^5.3.2",
    "mjml": "^4.15.3",
    "nestjs-pino": "^4.0.0",
    "passport": "^0.7.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
