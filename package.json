    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/terminus": "^10.2.3",
    "@prisma/client": "5.9.1",
    "@sendgrid/mail": "^8.1.1",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
